//mixedArr  Array
let mixedArr = ["string", ["otherarray"], 123, true];
console.log(mixedArr[0]); // Outputs: string
console.log(mixedArr[1]); // Outputs: ['otherarray']
console.log(mixedArr[2]); // Outputs: 123
console.log(mixedArr[3]); // Outputs: true
